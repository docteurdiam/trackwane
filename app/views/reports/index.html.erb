<%= render :partial => "listing" %>
<%= render :partial => "parameters" %>
<%= render :partial => "presentation" %>

<div id="canvas">
  <div class="presentation"></div>
  <div class="parameters"></div>
  <div class="sidebar">
    <div class="listing lionbars"></div>
    <button class="run blue-button"><div>Run Report</div></button>
  </div>
  <div class="toolbar">
    <div class='button pdf'>
      <img src='/assets/103-map.png'><label>Export PDF</label>
    </div>
    <div class='button excel'>
      <img src='/assets/103-map.png'><label>Export Excel</label>
    </div>
    <div class='button data'>
      <img src='/assets/103-map.png'><label>Show Data</label>
    </div>
  </div>
</div>

<script type="text/javascript">
  $(function() {
    var controller = new App.Controllers.ReportsController({el: "#canvas"});
  });
</script>
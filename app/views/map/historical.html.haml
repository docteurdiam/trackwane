:javascript
  $(function() {
    Ext.application({
      name: 'Gowane',
      autoCreateViewport: false,
      controllers: ['Gowane.controllers.Historical'],
      launch: function() {
        Ext.create('Gowane.stores.Devices', {
          storeId: "DeviceStore"
        });
        Ext.create('Gowane.stores.Groups', {
          storeId: "GroupStore"
        });
        new Ext.Viewport({
          layout : 'border',
          items : [
            { xtype: 'menu', region : 'north'},
            { region : 'west', width: 900, xtype: 'historical_map', split: true},
            { xtype: 'sharedsidecolumn', region: 'center', layout: 'vbox', layoutConfig: {align: 'stretch'},
              items: [
                {xtype: "date_selection", width: 300},
                {xtype: 'summary_device_list', width: 300},
                {xtype: 'summary_group_list', width: 300}]
            }
          ]
        });
      }
    });
  });

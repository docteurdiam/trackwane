:javascript
  $(function() {
    $.App.Help = "historical";
    Ext.application({
      name: 'Gowane',
      autoCreateViewport: false,
      controllers: ['Gowane.controllers.Historical'],
      launch: function() {
        Ext.create('Gowane.stores.Devices', {
          storeId: "DeviceStore"
        });
        Ext.create('Gowane.stores.Groups', {
          storeId: "GroupStore"
        });
        Ext.create('Gowane.stores.GpsEvents', {
          storeId: "GpsEventStore"
        });
        new Ext.Viewport({
          layout : 'border', items : [
            {xtype: 'menu', region : 'north'},
            {xtype: 'panel', region: 'center', layout: {type: 'vbox', pack: 'start', align: 'stretch'}, items: [
              {xtype: 'panel', flex: 2, layout: {type: 'hbox', pack: 'start', align: 'stretch'}, items: [
                {flex: 3, xtype: 'historical_map', split: true},
                {xtype: 'sharedsidecolumn', flex: 1, layout: {type: 'vbox', pack: 'start', align: 'stretch'}, items: [
                  {xtype: "date_selection"},
                  {xtype: 'summary_device_list'}
                ]}
              ]},
              {xtype: 'full_gps_event_list', flex: 1}
            ]}
          ]
        });
      }
    });
  });

<script id="place-listing-template" type="text/x-handlebars-template">
  <table id="places" class="sidecolumn">
      <tr>
        <th><input type="checkbox" /></th>
        <th><%= t("places.place") %></th>
        <th><%= t("places.category") %></th>
        <th></th>
      </tr>
    {{#each models}}
      <tr>
        <td>
          <input type="checkbox" data-id="{{attributes.id}}"/>
        </td>
        <td>
          <a data-id="{{attributes.id}}" class="select" href="#">{{attributes.name}}</a>
        </td>
        <td>{{attributes.category}}</td>
        <td style="width: 10px"><a class="remove" href="#" data-id="{{attributes.id}}"><img src="/assets/closelabel.png" /></a></td>
      </tr>
      {{/each}}
  </table>
</script>

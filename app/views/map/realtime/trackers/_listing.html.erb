<script id="tracker-listing-template" type="text/x-handlebars-template">
  <table>
    <tbody>
      {{#each models}}
        <tr data-id="{{attributes.device_id}}">
          <td>
            <img style="float: left; margin-top: 4px; margin-right: 10px; border: 1px solid #ccc" width="30" height="30" src="/assets/unknown.png" />
              <a href="#" data-id="{{attributes.id}}" style="font-size: 10px; font-family: Verdana; font-weight: bold">{{attributes.name}}</a><br/>
              <i style="font-size: 10px" class="address">{{attributes.address}}</i>
              <br/>
              {{#if attributes.place}}
                <i class="place">{{attributes.place.name}}</i>
              {{else}}
                <i class="place"></i>
              {{/if}}
          </td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</script>
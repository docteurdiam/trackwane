<script id="realtime-device-template" type="text/x-handlebars-template">
  <table id="realtime-devices" class="sidecolumn">
    {{#each models}}
      <tr>
        <td>
          <div style="display: inline-block; margin-left: 5px; width: 180px">
            <a href="#" data-id="{{attributes.id}}" style="font-size: 10px; font-family: Verdana; font-weight: bold">{{attributes.name}}</a><br/>
            <i style="font-size: 10px">{{attributes.address}} <br/>50m de Treichville Centre</i>
          </div>
          <div style="display: inline-block; width: 70px">
            {{attributes.speed}} km/h<br/>
            3.2L
          </div>
        </td>
      </tr>
      {{/each}}
  </table>
</script>
<%= render :partial => "speed_alarms" %>
<%= render :partial => "new_speed_alarm" %>
<%= render :partial => "geofence_alarms" %>
<%= render :partial => "new_geofence_alarm" %>

<div id="canvas">
  <div id="map" style="width: 100%; height: 100%"></div>
  <div id="alarm-listing" class="listing">
    <div class="speed-alarms"></div>
    <div class="geofence-alarms"></div>
  </div>

  <div id="new-speed-alarm-panel">
  </div>

  <div id="new-geofence-alarm-panel">
  </div>

  <div class="toolbar">
    <div class='button new-speed-alarm'>
      <img src='/assets/103-map.png'><label>Show Speed Alarms</label>
    </div>
    <div class='button new-geofence-alarm'>
      <img src='/assets/103-map.png'><label>Show Geofence Alarms</label>
    </div>
  </div>

</div>

<script type="text/javascript">
  $(function() {
    new App.Controllers.AlarmsController({el: "#canvas"});
  });
</script>

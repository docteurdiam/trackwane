:javascript
  $(function() {
    $.App.help = "places";
    $.App.account_id = #{session[:account_id]};
    Ext.application({
      name: 'Gowane',
      autoCreateViewport: false,
      controllers: ['Gowane.controllers.Places'],
      launch: function() {
        new Ext.Viewport({
          layout : 'border',
          items : [
            { xtype: 'menu', region : 'north'},
            { region : 'west', width: 900, xtype: 'placesmap', split: true},
            { xtype: 'sharedsidecolumn', region: 'center', layout: 'fit', layoutConfig: {align: 'stretch'},
              items: [{xtype: 'summary_place_list', width: 300}]
            }
          ]
        });
      }
    });
  });

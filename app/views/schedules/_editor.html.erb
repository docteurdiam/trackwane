<script id="editor-template" type="text/x-handlebars-template">
  <fieldset>
    <label>Report</label>
    <input name="report_id" value="{{attributes.report_id}}" type="text" />
    <label>Format</label>
    <input name="format" value="{{attributes.format}}" type="text" />
    <label>Email</label>
    <input value="{{attributes.email}}" type="text" />
    <label>Frequency</label>
    <input value="{{attributes.frequency}}" type="text" />
    {{#if attributes}}
      <input  value="{{attributes.id}}" type="hidden" name="id" />
      <button style="margin-top: 15px; margin-bottom: 5px; float: right" class="save blue-button"><div>Accept</div></button>
      <button style="margin-top: 15px; margin-bottom: 5px; float: right; margin-right: 10px" class="delete blue-button"><div>Delete</div></button>
    {{else}}
      <button style="margin-top: 15px; margin-bottom: 5px; float: right" class="blue-button create"><div>Accept</div></button>
    {{/if}}
  </fieldset>
</script>
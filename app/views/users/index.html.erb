<div id="canvas">
    <div class="listing" style="float: left; margin: auto; margin-top: 100px; left: 80px; width: 800px;">
      <table>
        <thead>
          <th>Login</th>
          <th>Email</th>
          <th>Role</th>
          <th>Last Login</th>
        </thead>
      <% for user in @users %>
        <tr>
          <td><%= user.login %></td>
          <td><%= user.email %></td>
          <td>Administrator</td>
          <td>Never</td>
        </tr>
      <% end %>
      </table>
    </div>
    <div class="editor" style=" float: left; width: 300px; margin: auto; margin-top: 100px; right: 20px">
      <fieldset>
        <label>Username</label>
        <input type="text" />
        <label>Email</label>
        <input type="text" />
        <label>Role</label>
        <input type="text" />
        <label>Password</label>
        <input type="text" />
        <label>Password Confirmation</label>
        <input type="text" />
      </fieldset>
    </div>
</div>


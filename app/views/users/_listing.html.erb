<script id="listing-template" type="text/x-handlebars-template">
      <table>
        <thead>
          <th>Login</th>
          <th>Email</th>
          <th>Role</th>
          <th></th>
        </thead>
  {{#each models}}
    <tr data-id="{{attributes.id}}">
          <td class="selectable">{{attributes.email}}</td>
          <td class="selectable">{{attributes.login}}</td>
          <td class="selectable">{{attributes.role}}</td>
          <td style="width: 10px"><a data-id="{{attributes.id}}"  class="remove" href="#"><img src="/assets/closelabel.png" /></a></td>
        </tr>
    {{/each}}
      </table>
</script>